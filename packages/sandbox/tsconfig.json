{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "target": "ES2017"
  },
  "include": [
    "src/index.tsx",
    "src/App.tsx",
    "src/serviceWorker.ts",
    "src/App.test.tsx",
    "src/test-utils.tsx",
    "src/constants/transactions/LitecoinTransactions.ts",
    "src/constants/transactions/AvalancheTransactions.ts",
    "src/constants/transactions/ThorchainTransactions.ts",
    "src/constants/transactions/SecretTransactions.ts",
    "src/constants/transactions/BitcoinTransactions.ts",
    "src/constants/transactions/BitcoinCashTransactions.ts",
    "src/constants/transactions/OsmosisTransactions.ts",
    "src/constants/transactions/CosmosTransactions.ts",
    "src/constants/transactions/TerraTransactions.ts",
    "src/constants/transactions/index.ts",
    "src/constants/transactions/KavaTransactions.ts",
    "src/constants/transactions/EthereumTransactions.ts",
    "src/constants/transactions/DogecoinTransactions.ts",
    "src/constants/transactions/BinanceTransactions.ts",
    "src/setupTests.ts",
    "src/reportWebVitals.ts",
    "src/components/SideBarItem/SideBarItem.tsx",
    "src/components/SideBarItemList/SideBarItemList.tsx",
    "src/components/Card/Card.tsx",
    "src/components/Card/InputGroup.tsx",
    "src/components/Card/SelectActionGroup.tsx",
    "src/components/Card/OutputGroup.tsx",
    "src/components/ConnectButton/ConnectButton.tsx",
    "src/components/MetaMaskCardList/MetaMaskCardListConfig.ts",
    "src/components/AssetCardList/AssetCardListConfig.ts",
    "src/components/Layout/SideBar/SideBar.tsx",
    "src/components/Layout/Footer/Footer.tsx",
    "src/components/Layout/Main/Main.tsx",
    "src/components/Layout/Header/Header.tsx",
    "src/components/CardList/CardList.tsx",
    "src/theme/colors.ts",
    "src/theme/breakpoints.ts",
    "src/theme/fonts.ts",
    "src/theme/styles.ts",
